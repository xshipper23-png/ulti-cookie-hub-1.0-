    Name = "Teleport to M9",
    Callback = function()
        if firstM9 and localPlayer.Character and localPlayer.Character:FindFirstChild("HumanoidRootPart") then
            local targetPos = firstM9:IsA("Model") and firstM9:GetModelCFrame() or firstM9.CFrame
            localPlayer.Character.HumanoidRootPart.CFrame = targetPos + Vector3.new(0, 3, 0)
            Rayfield:Notify({
                Title = "Teleport Success",
                Content = "Teleported to M9!",
                Duration = 3,
                Image = 4483362458,
            })
        else
            Rayfield:Notify({
                Title = "Error",
                Content = "No M9 found on the map.",
                Duration = 3,
                Image = 4483362458,
            })
        end
    end,
})

