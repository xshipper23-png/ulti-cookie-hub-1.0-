})

-- --- TRACKER LOOP (Runs every second) ---
task.spawn(function()
    while true do
        local foundM9 = {}
        local foundKeys = {}
        
        -- Scanning Workspace for M9 and Key card
        for _, obj in pairs(workspace:GetDescendants()) do
            if obj.Name == "M9" then
                table.insert(foundM9, obj)
            elseif obj.Name == "Key card" then
                table.insert(foundKeys, obj)
            end
        end
        
        -- Notification Logic
        if notifyEnabled then
            if #foundM9 > m9Count and #foundM9 > 0 then
                Rayfield:Notify({
                    Title = "Item Detected!",
                    Content = "An M9 has been spotted on the map!",
                    Duration = 5,
                    Image = 4483362458,
                })
            end
            if #foundKeys > keyCardCount and #foundKeys > 0 then
                Rayfield:Notify({
                    Title = "Item Detected!",
                    Content = "A Key card has been spotted on the map!",
                    Duration = 5,
                    Image = 4483362458,
                })
            end
        end

        -- Update counts and references
        m9Count = #foundM9
        keyCardCount = #foundKeys
        firstM9 = foundM9[1]
        firstKeyCard = foundKeys[1]
        
        -- Update Labels with Status and Color
        local m9Text = "M9: " .. m9Count .. (m9Count > 0 and " (Exist)" or " (Not found)")
        local m9Color = m9Count > 0 and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 100, 100)
        m9Label:Set(m9Text, 4483362458, m9Color, false)
        
        local keyText = "Key card: " .. keyCardCount .. (keyCardCount > 0 and " (Exist)" or " (Not found)")
        local keyColor = keyCardCount > 0 and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 100, 100)
        keyLabel:Set(keyText, 4483362458, keyColor, false)
        
        task.wait(1)
    end
end)

