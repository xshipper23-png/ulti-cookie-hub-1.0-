---------------------------------globalTab-----------------------------

local players = game:GetService("Players")
local teleportService = game:GetService("TeleportService")
local runService = game:GetService("RunService")
local userInputService = game:GetService("UserInputService")
local localPlayer = players.LocalPlayer
local camera = workspace.CurrentCamera

-- --- VARIABLEN ---
local ghostActive = false
local ghostTargetPos = Vector3.new(-938, 97, 1915) 
local camSpeed = 1.5
local rotationX = 0
local rotationY = 0

-- --- SERVER UTILITIES ---
globalTab:CreateSection("Server Utilities")

globalTab:CreateButton({
    Name = "Rejoin Server",
    Callback = function()
        teleportService:Teleport(game.PlaceId, localPlayer)
    end,
})

globalTab:CreateButton({
    Name = "Server Hop",
    Callback = function()
        local HttpService = game:GetService("HttpService")
        local api = "https://games.roblox.com/v1/games/" .. game.PlaceId .. "/servers/Public?sortOrder=Asc&limit=100"
        local success, result = pcall(function()
            return HttpService:JSONDecode(game:HttpGet(api))
        end)
        
        if success and result.data then
            for _, server in pairs(result.data) do
                if server.playing < server.maxPlayers and server.id ~= game.JobId then
                    teleportService:TeleportToPlaceInstance(game.PlaceId, server.id)
                    break
                end
            end
        end
    end,
})

globalTab:CreateToggle({
    Name = "Anti-AFK",
    CurrentValue = false,
    Flag = "global_antiafk",
    Callback = function(Value)
        localPlayer.Idled:Connect(function()
            if Value then
                local vu = game:GetService("VirtualUser")
                vu:Button2Down(Vector2.new(0,0), camera.CFrame)
                task.wait(1)
                vu:Button2Up(Vector2.new(0,0), camera.CFrame)
            end
        end)
    end,
})

-- --- GHOST MODE SECTION ---
globalTab:CreateSection("Ghost Mode")

globalTab:CreateToggle({
    Name = "Enable Ghost Mode",
    CurrentValue = false,
    Flag = "global_ghost_final",
    Callback = function(Value)
        ghostActive = Value
        local char = localPlayer.Character
        local root = char and char:FindFirstChild("HumanoidRootPart")
        local hum = char and char:FindFirstChildOfClass("Humanoid")
        
        if ghostActive and root and hum then
            -- Initialisiere Rotation mit aktueller Kamera-Sicht
            local look = camera.CFrame.LookVector
            rotationX = math.atan2(-look.X, -look.Z)
            rotationY = math.asin(look.Y)

            camera.CameraType = Enum.CameraType.Scriptable
            root.CFrame = CFrame.new(ghostTargetPos)
            task.wait(0.1)
            root.Anchored = true
            
            Rayfield:Notify({Title = "Ghost Mode", Content = "Use WASD to fly, Right-Click to look!", Duration = 4})
        elseif not ghostActive and root then
            -- RESET
            root.Anchored = false
            root.CFrame = camera.CFrame
            
            camera.CameraType = Enum.CameraType.Custom
            camera.CameraSubject = hum
            userInputService.MouseBehavior = Enum.MouseBehavior.Default
            
            Rayfield:Notify({Title = "Ghost Mode", Content = "Camera & Character Restored!", Duration = 4})
        end
    end,
})

globalTab:CreateSlider({
    Name = "Ghost Camera Speed",
    Range = {0.5, 5},
    Increment = 0.1,
    CurrentValue = 1.5,
    Callback = function(Value)
        camSpeed = Value
    end,
})

-- --- GHOST CONTROLS LOOP ---
runService.RenderStepped:Connect(function()
    if ghostActive then
        -- Rechtsklick Logik zum Umsehen
        if userInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton2) then
            userInputService.MouseBehavior = Enum.MouseBehavior.LockCurrentPosition
            local delta = userInputService:GetMouseDelta()
            rotationX = rotationX - delta.X * 0.005
            rotationY = math.clamp(rotationY - delta.Y * 0.005, -math.rad(80), math.rad(80))
        else
            userInputService.MouseBehavior = Enum.MouseBehavior.Default
        end
        
        -- Kamera-Rotation berechnen
        camera.CFrame = CFrame.new(camera.CFrame.Position) * CFrame.Angles(0, rotationX, 0) * CFrame.Angles(rotationY, 0, 0)

        -- Bewegung
        local moveDir = Vector3.new(0,0,0)
        if userInputService:IsKeyDown(Enum.KeyCode.W) then moveDir = moveDir + camera.CFrame.LookVector end
        if userInputService:IsKeyDown(Enum.KeyCode.S) then moveDir = moveDir - camera.CFrame.LookVector end
        if userInputService:IsKeyDown(Enum.KeyCode.A) then moveDir = moveDir - camera.CFrame.RightVector end
        if userInputService:IsKeyDown(Enum.KeyCode.D) then moveDir = moveDir + camera.CFrame.RightVector end
        if userInputService:IsKeyDown(Enum.KeyCode.E) then moveDir = moveDir + Vector3.new(0, 1, 0) end
        if userInputService:IsKeyDown(Enum.KeyCode.Q) then moveDir = moveDir - Vector3.new(0, 1, 0) end
        
        camera.CFrame = camera.CFrame + (moveDir * camSpeed)
    end
end)

local network = settings().Network
local blinkEnabled = false
local autoSyncEnabled = false
local blinkIntensity = 10 -- Standardwert

-- --- FUNKTION: SYNC STEUERUNG ---
local function setNetworkLag(state)
    if state then
        network.IncomingReplicationLag = blinkIntensity
    else
        network.IncomingReplicationLag = 0
    end
end

-- --- AUTO-SYNC LOGIK (BEIM SCHIESSEN) ---
game:GetService("UserInputService").InputBegan:Connect(function(input, processed)
    if processed or not (blinkEnabled and autoSyncEnabled) then return end
    
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        setNetworkLag(false) 
        task.wait(0.2) -- Etwas längeres Fenster für den 10-Sekunden-Fix
        if blinkEnabled then
            setNetworkLag(true)
        end
    end
end)

-- --- GUI ELEMENTE ---
globalTab:CreateSection("Network Blink V2 (Stability Fix)(work not perfect)")

globalTab:CreateToggle({
    Name = "Enable Blink",
    CurrentValue = false,
    Callback = function(Value)
        blinkEnabled = Value
        setNetworkLag(Value)
    end,
})

globalTab:CreateToggle({
    Name = "Auto-Sync on Shoot",
    CurrentValue = false,
    Callback = function(Value)
        autoSyncEnabled = Value
    end,
})

globalTab:CreateSlider({
    Name = "Blink Intensity (Lag)",
    Range = {1, 30},
    Increment = 1,
    CurrentValue = 10,
    Callback = function(Value)
        blinkIntensity = Value
        if blinkEnabled then setNetworkLag(true) end
    end,
})

globalTab:CreateParagraph({
    Title = "10s Fix Info", 
    Content = "Wenn der Server dich alle 10s teleportiert, setze die Intensity niedriger (ca. 5-8) oder klicke öfter mal die linke Maustaste, um den Timer zu resetten."
})

