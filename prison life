            local targetPos = firstKeyCard:IsA("Model") and firstKeyCard:GetModelCFrame() or firstKeyCard.CFrame
            localPlayer.Character.HumanoidRootPart.CFrame = targetPos + Vector3.new(0, 3, 0)
