local runService = game:GetService("RunService")
local localPlayer = game.Players.LocalPlayer

-- --- VARIABLEN FÜR DIE LOGIK ---
local antiSlowEnabled = false
local antiSlowMode = "Use PlayerTab Speed"
local antiSlowSpeed = 16
local antiSlowDelay = 1

-- --- GUI ELEMENTE IN STTAB ---

stTab:CreateSection("Anti-Slow / Anti-Taser")

stTab:CreateToggle({
    Name = "Enable Anti-Slow",
    CurrentValue = false,
    Flag = "st_antislow_toggle",
    Callback = function(Value)
        antiSlowEnabled = Value
    end,
})

stTab:CreateDropdown({
    Name = "Anti-Slow Mode",
    Options = {"Use PlayerTab Speed", "Use Slider Speed", "Slider then PlayerTab"},
    CurrentOption = {"Use PlayerTab Speed"},
    MultipleOptions = false,
    Flag = "st_antislow_mode",
    Callback = function(Option)
        antiSlowMode = Option[1]
    end,
})

stTab:CreateSlider({
    Name = "Anti-Slow Speed",
    Range = {0, 100},
    Increment = 1,
    CurrentValue = 16,
    Flag = "st_antislow_speed",
    Callback = function(Value)
        antiSlowSpeed = Value
    end,
})

stTab:CreateSlider({
    Name = "Anti-Slow Delay (Mixed Mode)",
    Range = {0, 5},
    Increment = 0.1,
    CurrentValue = 1,
    Flag = "st_antislow_delay",
    Callback = function(Value)
        antiSlowDelay = Value
    end,
})

-- --- ANTI-SLOW LOGIK LOOP ---
runService.Heartbeat:Connect(function()
    if not antiSlowEnabled then return end
    
    local char = localPlayer.Character
    local hum = char and char:FindFirstChildOfClass("Humanoid")
    
    if hum then
        -- Wenn WalkSpeed durch einen Effekt auf 0 oder sehr niedrig gesetzt wird
        if hum.WalkSpeed <= 0.1 then
            
            if antiSlowMode == "Use PlayerTab Speed" then
                -- Nutzt den Wert, den du im Player Tab eingestellt hast
                hum.WalkSpeed = _G.plTabSpeed or 16 
                
            elseif antiSlowMode == "Use Slider Speed" then
                -- Nutzt fest den Speed vom Slider hier im stTab
                hum.WalkSpeed = antiSlowSpeed
                
            elseif antiSlowMode == "Slider then PlayerTab" then
                -- Kurzzeitig Slider Speed, dann zurück zum Player Tab Speed
                hum.WalkSpeed = antiSlowSpeed
                task.wait(antiSlowDelay)
                hum.WalkSpeed = _G.plTabSpeed or 16
            end
            
        end
    end
end)



---------------------------------comTab--------------------------------

local Section = comTab:CreateSection("visuel")

local players = game:GetService("Players")
local runService = game:GetService("RunService")
local localPlayer = players.LocalPlayer

local espAktiv = false

-- --- 1. HAUPTFUNKTION: ESP LOGIK ---
local function createESP(player)
    if player == localPlayer then return end

    local function setupCharacter(char)
        local root = char:WaitForChild("HumanoidRootPart", 10)
        local hum = char:WaitForChild("Humanoid", 10)
        if not root or not hum then return end

        if root:FindFirstChild("AdminESP") then root.AdminESP:Destroy() end

        -- BillboardGui
        local bgui = Instance.new("BillboardGui")
        bgui.Name = "AdminESP"
        bgui.Adornee = root
        bgui.Size = UDim2.new(4, 0, 5.5, 0)
        bgui.AlwaysOnTop = true
        bgui.ClipsDescendants = false -- WICHTIG: Erlaubt Anzeigen außerhalb der Box
        bgui.ResetOnSpawn = false
        bgui.Parent = root

        -- Haupt-Rahmen
        local box = Instance.new("Frame")
        box.Name = "ESPBox"
        box.Size = UDim2.new(1, 0, 1, 0)
        box.BackgroundTransparency = 1
        box.ClipsDescendants = false -- WICHTIG
        box.Parent = bgui

        local stroke = Instance.new("UIStroke")
        stroke.Thickness = 2
        stroke.Color = player.TeamColor.Color
        stroke.Parent = box

        -- NAME & DISTANZ (In die Box verschoben für maximale Sicherheit)
        local info = Instance.new("TextLabel")
        info.Name = "NameDistLabel"
        info.Size = UDim2.new(1, 0, 0.2, 0)
        info.Position = UDim2.new(0, 0, 0, 5) -- Innerhalb der Box oben
        info.BackgroundTransparency = 1
        info.TextColor3 = Color3.new(1, 1, 1)
        info.TextStrokeTransparency = 0 
        info.TextStrokeColor3 = Color3.new(0, 0, 0)
        info.Font = Enum.Font.GothamBold
        info.TextScaled = true
        info.ZIndex = 10
        info.Parent = box

        -- HP BAR HINTERGRUND (In die Box verschoben)
        local barBack = Instance.new("Frame")
        barBack.Name = "HealthBarBack"
        barBack.Size = UDim2.new(0.05, 0, 0.8, 0)
        barBack.Position = UDim2.new(0.02, 0, 0.1, 0) -- Innerhalb der Box links
        barBack.BackgroundColor3 = Color3.new(0, 0, 0)
        barBack.BorderSizePixel = 0
        barBack.ZIndex = 8
        barBack.Parent = box
        
        -- HP BAR FÜLLUNG
        local barFill = Instance.new("Frame")
        barFill.Name = "HealthBarFill"
        barFill.Size = UDim2.new(1, 0, 1, 0)
        barFill.BackgroundColor3 = Color3.new(0, 1, 0)
        barFill.BorderSizePixel = 0
        barFill.ZIndex = 9
        barFill.Parent = barBack

        -- UPDATE SCHLEIFE
        local connection
        connection = runService.RenderStepped:Connect(function()
            if not espAktiv or not char.Parent or not root.Parent then
                bgui:Destroy()
                connection:Disconnect()
                return
            end

            -- Text Update
            local dist = math.floor((root.Position - localPlayer.Character.HumanoidRootPart.Position).Magnitude)
            info.Text = player.Name .. " (" .. dist .. " studs)"
            
            -- HP Bar Update
            local hp = math.clamp(hum.Health / hum.MaxHealth, 0, 1)
            barFill.Size = UDim2.new(1, 0, hp, 0)
            barFill.Position = UDim2.new(0, 0, 1 - hp, 0)
            barFill.BackgroundColor3 = Color3.fromHSV(hp * 0.3, 1, 1)
        end)
    end

    player.CharacterAdded:Connect(setupCharacter)
    if player.Character then setupCharacter(player.Character) end
end

-- --- 2. INITIALISIERUNG ---
players.PlayerAdded:Connect(function(player)
    if espAktiv then createESP(player) end
end)

-- --- 3. TOGGLE ---
local Toggle = comTab:CreateToggle({
   Name = "ESP (Name/HP/Distanz)",
   CurrentValue = false,
   Flag = "esp_toggle_final",
   Callback = function(Value)
        espAktiv = Value
        if espAktiv then
            for _, p in pairs(players:GetPlayers()) do createESP(p) end
        else
            for _, p in pairs(players:GetPlayers()) do
                if p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
                    local gui = p.Character.HumanoidRootPart:FindFirstChild("AdminESP")
                    if gui then gui:Destroy() end
                end
            end
        end
   end,
})

local Section = comTab:CreateSection("aim bot")

local players = game:GetService("Players")
local runService = game:GetService("RunService")
local userInputService = game:GetService("UserInputService")
local localPlayer = players.LocalPlayer
local camera = workspace.CurrentCamera

-- --- VARIABLEN ---
local aimbotEnabled = false
local aimbotMode = "Closest"
local fovRadius = 100
local aimSmoothness = 0.2
local aimKey = Enum.UserInputType.MouseButton2

-- Neue Toggle-Variablen
local wallCheck = false
local teamCheck = false
local deadCheck = true -- Standardmäßig an, da man selten auf Tote zielt

-- --- FOV CIRCLE ---
local fovCircle = Drawing.new("Circle")
fovCircle.Visible = false
fovCircle.Thickness = 1
fovCircle.Color = Color3.fromRGB(255, 255, 255)
fovCircle.Transparency = 0.7
fovCircle.NumSides = 64

-- --- HILFSFUNKTION: WALL CHECK ---
local function isVisible(part)
    if not wallCheck then return true end
    
    local ray = Ray.new(camera.CFrame.Position, (part.Position - camera.CFrame.Position).Unit * 500)
    local hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {localPlayer.Character, camera})
    
    if hit and hit:IsDescendantOf(part.Parent) then
        return true
    end
    return false
end

-- --- LOGIK: ZIEL FINDEN ---
local function getTarget()
    local target = nil
    local shortestDist = math.huge
    local mousePos = userInputService:GetMouseLocation()

    for _, p in pairs(players:GetPlayers()) do
        if p ~= localPlayer and p.Character and p.Character:FindFirstChild("Head") and p.Character:FindFirstChild("Humanoid") then
            
            -- 1. Dead Check
            if deadCheck and p.Character.Humanoid.Health <= 0 then continue end
            
            -- 2. Team Check
            if teamCheck and p.Team == localPlayer.Team then continue end

            local head = p.Character.Head
            local pos, onScreen = camera:WorldToViewportPoint(head.Position)
            
            if onScreen then
                -- 3. Wall Check
                if not isVisible(head) then continue end

                local screenPos = Vector2.new(pos.X, pos.Y)
                local distToMouse = (screenPos - mousePos).Magnitude

                if distToMouse <= fovRadius then
                    if aimbotMode == "Closest" then
                        if distToMouse < shortestDist then
                            target = p
                            shortestDist = distToMouse
                        end
                    elseif aimbotMode == "Lowest Health" then
                        if p.Character.Humanoid.Health < shortestDist then
                            target = p
                            shortestDist = p.Character.Humanoid.Health
                        end
                    end
                end
            end
        end
    end
    return target
end

-- --- AIMBOT LOOP ---
runService.RenderStepped:Connect(function()
    fovCircle.Position = userInputService:GetMouseLocation()
    fovCircle.Radius = fovRadius
    fovCircle.Visible = aimbotEnabled

    if aimbotEnabled and userInputService:IsMouseButtonPressed(aimKey) then
        local target = getTarget()
        if target and target.Character and target.Character.Head then
            local targetPos = target.Character.Head.Position
            local currentCF = camera.CFrame
            local targetCF = CFrame.new(currentCF.Position, targetPos)
            camera.CFrame = currentCF:Lerp(targetCF, aimSmoothness)
        end
    end
end)

-- --- GUI ELEMENTE IN COMTAB ---

comTab:CreateToggle({
    Name = "Enable Aimbot",
    CurrentValue = false,
    Flag = "aim_enabled",
    Callback = function(Value) aimbotEnabled = Value end,
})

comTab:CreateToggle({
    Name = "Wall Check",
    CurrentValue = false,
    Flag = "aim_wallcheck",
    Callback = function(Value) wallCheck = Value end,
})

comTab:CreateToggle({
    Name = "Team Check",
    CurrentValue = false,
    Flag = "aim_teamcheck",
    Callback = function(Value) teamCheck = Value end,
})

comTab:CreateToggle({
    Name = "Dead Check",
    CurrentValue = true,
    Flag = "aim_deadcheck",
    Callback = function(Value) deadCheck = Value end,
})

comTab:CreateDropdown({
    Name = "Target Mode",
    Options = {"Closest", "Lowest Health"},
    CurrentOption = {"Closest"},
    MultipleOptions = false,
    Flag = "aim_mode",
    Callback = function(Options) aimbotMode = Options[1] end,
})

comTab:CreateSlider({
    Name = "FOV Radius",
    Range = {10, 800},
    Increment = 10,
    Suffix = " Pixels",
    CurrentValue = 100,
    Flag = "aim_fov",
    Callback = function(Value) fovRadius = Value end,
})

comTab:CreateSlider({
    Name = "Aim Smoothness",
    Range = {1, 10},
    Increment = 1,
    Suffix = " (1 = Snap)",
    CurrentValue = 2,
    Flag = "aim_smooth",
    Callback = function(Value) aimSmoothness = Value / 10 end,
})

