   Name = "give MP5",
   Callback = function()
            
-- 1. Aktuelle Position des Spielers erfassen
local player = game.Players.LocalPlayer
local character = player.Character
local rootPart = character and character:FindFirstChild("HumanoidRootPart")

if rootPart then
    -- 2. Die jetzige Position (CFrame) in einer Variable speichern
    local gespeichertePos = rootPart.CFrame
    
    -- 3. Zu den Ziel-Koordinaten teleportieren
    rootPart.CFrame = CFrame.new(813, 103, 2229)
    task.wait(0.1)
    rootPart.CFrame = CFrame.new(813, 103, 2229)
    task.wait(0.1)
    rootPart.CFrame = CFrame.new(813, 103, 2229)
    task.wait(0.1)
    rootPart.CFrame = CFrame.new(813, 103, 2229)
    task.wait(0.1)
    rootPart.CFrame = CFrame.new(813, 103, 2229)
    task.wait(0.1)
    
    -- 5. Zurück zur gespeicherten Position teleportieren
    rootPart.CFrame = gespeichertePos
else
    warn("Charakter noch nicht geladen!")
end
            
   end,
})

