    if showCoords and localPlayer.Character and localPlayer.Character:FindFirstChild("HumanoidRootPart") then
        local pos = localPlayer.Character.HumanoidRootPart.Position
        coordsLabel:Set(string.format("Coords: X:%.1f Y:%.1f Z:%.1f", pos.X, pos.Y, pos.Z))
    elseif not showCoords then
        coordsLabel:Set("Coords: Hidden")
    end
end)

