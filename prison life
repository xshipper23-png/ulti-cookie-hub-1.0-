---------------------------------plTab---------------------------------

local players = game:GetService("Players")
local runService = game:GetService("RunService")
local userInputService = game:GetService("UserInputService")
local localPlayer = players.LocalPlayer
local camera = workspace.CurrentCamera

-- --- VARIABLEN ---
local walkSpeedValue = 16
local jumpPowerValue = 50
local wsEnabled = false
local infiniteJumpEnabled = false
local tpWalkEnabled = false
local tpWalkSpeed = 0.5
local anchorNoclip = false -- Die neue Methode
local flyEnabled = false
local flySpeed = 50

-- --- STEUERUNG FÜR FLY ---
local keysPressed = {}
userInputService.InputBegan:Connect(function(input, gpe)
    if not gpe then keysPressed[input.KeyCode] = true end
end)
userInputService.InputEnded:Connect(function(input)
    keysPressed[input.KeyCode] = false
end)

-- --- MASTER PHYSICS LOOP (Stepped) ---
runService.Stepped:Connect(function()
    local char = localPlayer.Character
    if not char then return end
    local root = char:FindFirstChild("HumanoidRootPart")
    local hum = char:FindFirstChildOfClass("Humanoid")
    if not root or not hum then return end

    -- 1. ANCHOR NOCLIP (Dein Wunsch)
    if anchorNoclip then
        root.Anchored = true -- Verankert dich, damit Wände dich nicht stoppen
        if hum.MoveDirection.Magnitude > 0 then
            -- Teleportiert dich trotz Verankerung in Laufrichtung
            root.CFrame = root.CFrame + (hum.MoveDirection * (walkSpeedValue/40))
        end
    else
        -- Wenn aus, dann Entankern (außer Fly ist an)
        if not flyEnabled then
            root.Anchored = false
        end
    end

    -- 2. TP WALK NOCLIP (Zusätzlich wie gewünscht)
    if tpWalkEnabled and not anchorNoclip and hum.MoveDirection.Magnitude > 0 then
        for _, part in pairs(char:GetDescendants()) do
            if part:IsA("BasePart") then part.CanCollide = false end
        end
        root.CFrame = root.CFrame + (hum.MoveDirection * tpWalkSpeed)
    end

    -- 3. FLY LOGIK
    if flyEnabled then
        root.Anchored = false -- Fly braucht Physik oder manuelle Steuerung
        local moveDir = Vector3.new(0, 0, 0)
        if keysPressed[Enum.KeyCode.W] then moveDir = moveDir + camera.CFrame.LookVector end
        if keysPressed[Enum.KeyCode.S] then moveDir = moveDir - camera.CFrame.LookVector end
        if keysPressed[Enum.KeyCode.A] then moveDir = moveDir - camera.CFrame.RightVector end
        if keysPressed[Enum.KeyCode.D] then moveDir = moveDir + camera.CFrame.RightVector end
        if keysPressed[Enum.KeyCode.Space] then moveDir = moveDir + Vector3.new(0, 1, 0) end
        if keysPressed[Enum.KeyCode.LeftControl] then moveDir = moveDir - Vector3.new(0, 1, 0) end

        if not root:FindFirstChild("FlyVelocity") then
            local bv = Instance.new("BodyVelocity", root)
            bv.Name = "FlyVelocity"
            bv.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
            local bg = Instance.new("BodyGyro", root)
            bg.Name = "FlyGyro"
            bg.MaxTorque = Vector3.new(math.huge, math.huge, math.huge)
        end
        root.FlyVelocity.Velocity = moveDir * flySpeed
        root.FlyGyro.CFrame = camera.CFrame
    else
        if root:FindFirstChild("FlyVelocity") then root.FlyVelocity:Destroy() end
        if root:FindFirstChild("FlyGyro") then root.FlyGyro:Destroy() end
    end
end)

-- --- REFRESH LOOP ---
runService.RenderStepped:Connect(function()
    local hum = localPlayer.Character and localPlayer.Character:FindFirstChildOfClass("Humanoid")
    if hum then
        if wsEnabled then hum.WalkSpeed = walkSpeedValue end
        if jumpPowerValue ~= 50 then
            hum.UseJumpPower = true
            hum.JumpPower = jumpPowerValue
        end
    end
end)

-- --- INFINITE JUMP ---
userInputService.JumpRequest:Connect(function()
    if infiniteJumpEnabled and localPlayer.Character then
        local hum = localPlayer.Character:FindFirstChildOfClass("Humanoid")
        if hum then hum:ChangeState(Enum.HumanoidStateType.Jumping) end
    end
end)

-- --- GUI ---

plTab:CreateSection("Movement")

local wsSlider = plTab:CreateSlider({
    Name = "WalkSpeed", Range = {16, 300}, Increment = 1, Suffix = " Speed", CurrentValue = 16, Flag = "ws_pl",
    Callback = function(v) walkSpeedValue = v wsEnabled = (v > 16) end
})

plTab:CreateButton({Name = "Reset Speed", Callback = function() wsEnabled = false wsSlider:Set(16) end})

plTab:CreateSection("Noclip Methods")

plTab:CreateToggle({
    Name = "Anchor Noclip (Ghost Walk)", 
    CurrentValue = false, 
    Flag = "anchor_nc", 
    Callback = function(v) anchorNoclip = v end
})

plTab:CreateToggle({
    Name = "TP Walk Noclip", 
    CurrentValue = false, 
    Flag = "tp_nc", 
    Callback = function(v) tpWalkEnabled = v end
})

plTab:CreateSection("Fly & Others")

plTab:CreateToggle({Name = "Fly", CurrentValue = false, Flag = "fly_pl", Callback = function(v) flyEnabled = v end})

plTab:CreateSlider({
    Name = "Fly Speed", Range = {10, 500}, Increment = 10, Suffix = " Speed", CurrentValue = 50, Flag = "fly_speed_pl",
    Callback = function(v) flySpeed = v end
})

plTab:CreateToggle({Name = "Infinite Jump", CurrentValue = false, Flag = "inf_j_pl", Callback = function(v) infiniteJumpEnabled = v end})


---------------------------------stTab---------------------------------
-------
-- --- SPEICHER & LOGIK FÜR TÜREN ---
local doorsStorage = {}
local removeDoorsEnabled = false
local doorsFolder = workspace:FindFirstChild("Doors")

-- Funktion zum Verstecken einer Tür
local function hideDoor(door)
    if not doorsStorage[door] then
        doorsStorage[door] = door.Parent
        door.Parent = nil
    end
end

-- Funktion zum Wiederherstellen aller Türen
local function restoreDoors()
    for door, originalParent in pairs(doorsStorage) do
        if door then
            door.Parent = originalParent
        end
    end
    doorsStorage = {}
end

-- Automatische Überwachung (falls neue Türen spawnen)
if doorsFolder then
    doorsFolder.ChildAdded:Connect(function(child)
        if removeDoorsEnabled then
            task.wait(0.1) -- Kurz warten, bis das Objekt voll geladen ist
            hideDoor(child)
        end
    end)
end

-- --- GUI ELEMENTE IN STTAB ---

stTab:CreateSection("Map Manipulation")

stTab:CreateToggle({
    Name = "Remove All Doors (Client-Side)",
    CurrentValue = false,
    Flag = "st_remove_doors",
    Callback = function(Value)
        removeDoorsEnabled = Value
        
        if removeDoorsEnabled then
            if doorsFolder then
                for _, door in pairs(doorsFolder:GetChildren()) do
                    hideDoor(door)
                end
                Rayfield:Notify({
                    Title = "Doors Removed",
                    Content = "Alle Türen wurden lokal versteckt.",
                    Duration = 2,
                    Image = 4483362458,
                })
            else
                Rayfield:Notify({
                    Title = "Fehler",
                    Content = "Ordner 'Doors' nicht gefunden!",
                    Duration = 3,
                    Image = 4483362458,
                })
            end
        else
            restoreDoors()
            Rayfield:Notify({
                Title = "Doors Restored",
                Content = "Alle Türen sind wieder da.",
                Duration = 2,
                Image = 4483362458,
            })
        end
    end,
})

