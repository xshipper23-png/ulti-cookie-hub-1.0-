local lighting = game:GetService("Lighting")
local runService = game:GetService("RunService")
local localPlayer = game.Players.LocalPlayer

-- --- STORAGE FOR XRAY ---
local xrayStorage = {}
local xrayEnabled = false

-- --- ATMOSPHERE SECTION ---
clientTab:CreateSection("Atmosphere")

clientTab:CreateToggle({
    Name = "Fullbright",
    CurrentValue = false,
    Flag = "cl_fullbright",
    Callback = function(Value)
        if Value then
            lighting.Ambient = Color3.fromRGB(255, 255, 255)
            lighting.Brightness = 2
            lighting.ExposureCompensation = 1
        else
            lighting.Ambient = Color3.fromRGB(127, 127, 127) -- Default
            lighting.Brightness = 1
            lighting.ExposureCompensation = 0
        end
    end,
})

clientTab:CreateToggle({
    Name = "No Fog",
    CurrentValue = false,
    Flag = "cl_nofog",
    Callback = function(Value)
        if Value then
            lighting.FogEnd = 1000000
        else
            lighting.FogEnd = 1000 -- Adjust this to game default if needed
        end
    end,
})

-- --- VISUAL SECTION ---
clientTab:CreateSection("Visual")

clientTab:CreateSlider({
    Name = "Field of View",
    Range = {70, 120},
    Increment = 1,
    CurrentValue = 70,
    Flag = "cl_fov",
    Callback = function(Value)
        workspace.CurrentCamera.FieldOfView = Value
    end,
})

clientTab:CreateToggle({
    Name = "X-Ray (50% Transparency)",
    CurrentValue = false,
    Flag = "cl_xray",
    Callback = function(Value)
        xrayEnabled = Value
        if xrayEnabled then
            for _, part in pairs(workspace:GetDescendants()) do
                if part:IsA("BasePart") and part.Transparency == 0 then
                    xrayStorage[part] = true
                    part.Transparency = 0.5
                end
            end
        else
            for part, _ in pairs(xrayStorage) do
                if part then part.Transparency = 0 end
            end
            xrayStorage = {}
        end
    end,
})

-- --- TOOLS SECTION ---
clientTab:CreateSection("Tools")

